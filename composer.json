{
  "name": "paulofelipem/laravel-brazilian-validators",
  "description": "Laravel - Biblioteca com validações de formatos e tipos Brasileiros",
  "license": "MIT",
  "homepage": "https://github.com/PauloFelipeM/laravel-brazilian-validators",
  "type": "library",
  "authors": [
    {
      "name": "Paulo Felipe Martins",
      "email": "paulofelipemartins@gmail.com",
      "role": "Developer"
