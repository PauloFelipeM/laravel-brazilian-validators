  "autoload-dev": {
    "psr-4": {}
  },
  "scripts": {},
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "PauloFelipeM\\BrazilianValidator\\BrazilianValidatorServiceProvider"
      ]
    }
  }
